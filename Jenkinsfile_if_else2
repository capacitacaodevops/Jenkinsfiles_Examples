pipeline{
    agent any
    environment{
     Toolname="jenkins"	
    }
    stages{    
	stage("Conditions") {
		steps{
			script{
        			if (Toolname=="jenkins"){
     				    sh '''
                                       var=$(cat jenkins.txt ) ; echo $var
                                       '''
					echo "Our CI CD tool"
				}
				else { 
					echo "Not CI CD tool"
        			}
    			}
		}
	}
     }
}
