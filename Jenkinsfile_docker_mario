pipeline {
    agent any
    
    stages {
               
        stage('Download Container mario') {
            steps {
                sh '''
                   id
                   pwd
                   docker-compose -f docker-compose.yml pull
                   '''
            }
         }
         
         stage('Iniciar Container Mario') {
            steps {
                sh '''
                   ./up_mario.sh
                   docker-compose -f docker-compose.yml up
                   '''
            }
         }
         
        
        
    }
}
