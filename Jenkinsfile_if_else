pipeline{
    agent any
    environment{
	Toolname="Jenkins"
    }
    stages{    
	stage("Conditions") {
		steps{
			script{
        			if (Toolname=="Jenkins"){
					echo "Our CI CD tool"
				}
				else { 
					echo "Not CI CD tool"
                                        sh 'touch arquivo.txt'
        			}
    			}
		}
	}
     }
}
